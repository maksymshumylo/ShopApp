<Window x:Class="Shop.Views.BucketWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shop.Views"
        xmlns:vms ="clr-namespace:Shop.ViewModels"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=vms:BucketViewModel}"
        Title="BucketWindow" Height="450" Width="800">
    <Grid>
        <TextBlock Text="{Binding Count, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</Window>
